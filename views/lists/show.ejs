<a href="/list/addItem" class="btn btn-success pull-right">New Item</a>

<h1>My List</h1>

<% items.forEach(function(item) { %>
<div class="well">
  <h2><a href="/lists/<%= item.id %>"><%= item.itemName %></a>
    <small><%= item.amount %></small>
    <!-- <small><%= item.userId %></small> -->
  </h2>
  <a href="/lists/<%= item.id %>/edit" class="btn btn-info glyphicon glyphicon-edit"></a>
  <a href="/lists/<%= item.id %>" class="btn btn-danger glyphicon glyphicon-trash delete-btn"></a>
</div>
<% }); %>





