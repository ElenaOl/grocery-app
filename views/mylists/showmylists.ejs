<h1>Here are your previously created lists</h1>

      <% names.forEach(function(name) { %>
        <div class="well">
            <h2><a href="/list/<%=name.id%>"><%= name.listName %></a></h2>
         
          
        </div>
        <% }); %>

      
      <form action="/list" method="POST">
        <div class="form-group">
          <label for="listName">Add New Grocery List</label>
          <input type="text" class="form-control" id="list" name="listName"required>
        </div>
      
        <input type="submit" class="btn btn-primary">
      </form>
      
      
      
      
